with "drivers/components/components.gpr";

library project OpenMV2 extends "drivers/ARM/STM32/stm32f42x.gpr" is

   for Library_Name use "openmv_board_support";
   for Library_Kind use "static";
   for Library_Dir use "lib";
   for Object_Dir use "obj";

   for Target use "arm-eabi";
   for Runtime ("Ada") use "ravenscar-full-stm32f429disco";

   for Source_Dirs use ("board");

   package Compiler renames STM32F42x.Compiler;
   package Builder renames STM32F42x.Builder;

end OpenMV2;
